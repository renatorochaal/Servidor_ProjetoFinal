<jasperReport name="subrelatorio_playlists">
    <!-- Configurações do sub-relatório -->

    <field name="id" class="java.lang.Integer"/>
    <field name="namePlaylist" class="java.lang.String"/>
    <field name="uriPlaylist" class="java.lang.String"/>

    <detail>
        <band height="50">
            <textField>
                <reportElement x="0" y="10" width="200" height="20"/>
                <textFieldExpression><![CDATA["ID da Playlist: " + $F{id}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="0" y="30" width="400" height="20"/>
                <textFieldExpression><![CDATA["Nome da Playlist: " + $F{namePlaylist}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="0" y="50" width="400" height="20"/>
                <textFieldExpression><![CDATA["URI da Playlist: " + $F{uriPlaylist}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
</jasperReport>
