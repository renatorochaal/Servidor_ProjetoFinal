<jasperReport name="subrelatorio_musicas_curtidas">
	<!-- Configurações do sub-relatório -->

	<field name="id" class="java.lang.Integer"/>
	<field name="nomeAlbum" class="java.lang.String"/>
	<field name="nomeMusica" class="java.lang.String"/>
	<field name="nomeDoArtista" class="java.lang.String"/>

	<detail>
		<band height="50">
			<textField>
				<reportElement x="0" y="10" width="400" height="20"/>
				<textFieldExpression><![CDATA["Música Curtida: " + $F{nomeDoArtista} + " - " + $F{nomeMusica} + " (Álbum: " + $F{nomeAlbum} + ")"]]></textFieldExpression>
			</textField>
		</band>
	</detail>

</jasperReport>
