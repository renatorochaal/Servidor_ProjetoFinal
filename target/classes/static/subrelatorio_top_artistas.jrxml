<jasperReport name="subrelatorio_top_artistas">
    <!-- Configurações do sub-relatório -->

    <field name="id" class="java.lang.Integer"/>
    <field name="nomeArtista" class="java.lang.String"/>
    <field name="imagemUrlArtista" class="java.lang.String"/>

    <detail>
        <band height="50">
            <textField>
                <reportElement x="0" y="10" width="200" height="20"/>
                <textFieldExpression><![CDATA["ID do Artista: " + $F{id}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="0" y="30" width="200" height="20"/>
                <textFieldExpression><![CDATA["Nome do Artista: " + $F{nomeArtista}]]></textFieldExpression>
            </textField>
            <image>
                <reportElement x="250" y="10" width="100" height="30"/>
                <imageExpression><![CDATA[$F{imagemUrlArtista}]]></imageExpression>
            </image>
        </band>
    </detail>
</jasperReport>
